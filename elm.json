        "ianmackenzie/elm-units": "2.2.0 <= v < 3.0.0"
    },
    "test-dependencies": {
        "elm/html": "1.0.0 <= v < 2.0.0",
        "elm/virtual-dom": "1.0.2 <= v < 2.0.0",
        "elm-explorations/test": "1.2.2 <= v < 2.0.0",
